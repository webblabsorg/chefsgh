{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/admin", "dest": "/index.html" },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
